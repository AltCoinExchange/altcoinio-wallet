{
  "name": "altcoinio-wallet",
  "version": "1.0.0",
  "description": "Wallet",
  "main": "dist/index.js",
  "typings": "dist/src/index.d.ts",
  "jam": {
    "main": "./dist/index.browser.js"
  },
  "browser": {
    "dist/index.js": "./dist/index.browser.js"
  },
  "scripts": {
    "brows": "browserify dist/index.js -o alo.js -t [ babelify --presets [ @babel/preset-es2015 ] ]",
    "uglify": "uglifyjs --mangle reserved=['BigInteger','ECPair','Point'] dist/index.browser.js -o dist/index.browser.js",
    "build": "webpack || echo not ok && npm run uglify",
    "test:watch": "npm test -- --watch",
    "coverage": "npm test -- --coverage --no-cache",
    "coverage:watch": "npm run coverage -- --watch",
    "prepare": "npm run build && npm test",
    "release": "standard-version && npm pack",
    "fix-bitcore": "./fix_scripts/fix_bitcore.sh",
    "postinstall": "postinstall-build . fix_scripts/fix_bitcore.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DxCx/ts-library-starter.git"
  },
  "keywords": [
    "example",
    "typescript",
    "library",
    "starter",
    "webpack",
    "browserify",
    "dts-bundle",
    "package",
    "tslint",
    "jest",
    "tsloader",
    "boilerplate",
    "ts-loader"
  ],
  "author": "altcoin.io",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/core": "7.0.0-beta.50",
    "@types/jest": "^20.0.7",
    "@types/node": "^8.0.24",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^8.0.0",
    "bigi": "^1.4.2",
    "browserify": "^16.2.3",
    "delete-empty": "^1.0.1",
    "dts-bundle": "^0.7.3",
    "jest": "^20.0.4",
    "standard-version": "^4.4.0",
    "ts-jest": "^20.0.14",
    "ts-loader": "^2.3.3",
    "tslint": "^5.6.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.4.2",
    "uglify-es": "^3.3.9",
    "uglify-js": "^3.3.4",
    "uglify-stream": "^1.1.0",
    "webpack": "^3.5.5",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "@types/bip32": "^1.0.0",
    "@types/bitcoinjs-lib": "^4.0.0",
    "augur-contracts": "^3.3.10",
    "axios": "^0.17.1",
    "bip39": "git+https://github.com/AltCoinExchange/bip39.git",
    "bitcoinjs-lib": "^4.0.2",
    "bitcore-mnemonic": "git+https://github.com/AltCoinExchange/bitcore-mnemonic.git",
    "clone": "^2.1.1",
    "codelyzer": "^4.0.2",
    "coinselect": "^3.1.11",
    "crypto-browserify": "^3.12.0",
    "postinstall-build": "^5.0.1",
    "reflect-metadata": "^0.1.10",
    "ripemd160": "^2.0.1",
    "uglifyify": "^5.0.1",
    "web3": "^1.0.0-beta.36",
    "web3-core-requestmanager": "^1.0.0-beta.34",
    "web3-eth-abi": "^1.0.0-beta.34",
    "web3-eth-accounts": "^1.0.0-beta.34",
    "web3-eth-contract": "^1.0.0-beta.34",
    "web3-eth-personal": "^1.0.0-beta.34",
    "web3-utils": "^1.0.0-beta.34"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "verbose": true,
    "mapCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coveragePathIgnorePatterns": [
      "/test/",
      "/node_modules/"
    ],
    "testEnvironment": "node",
    "testRegex": "test/.*\\.spec\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "@app/(.*)": "<rootDir>/src/$1"
    }
  }
}
