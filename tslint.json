{
	"extends": "tslint:recommended",
	"rules": {
		"max-line-length":200,
		"ordered-imports": false,
		"object-literal-sort-keys": false,
		"quotemark": true,
		"max-classes-per-file": [7],
		"radix": false,
		"no-console": false,
		"member-ordering": false,
		"object-literal-key-quotes": false,
		"trailing-comma": false,
		"eofline": false
	}
}
